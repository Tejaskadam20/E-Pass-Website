<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Register_Servlet</servlet-name>
        <servlet-class>Register_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApplyForEPass</servlet-name>
        <servlet-class>ApplyForEPass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Display_user</servlet-name>
        <servlet-class>Display_user</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Applied_For_EPass</servlet-name>
        <servlet-class>Applied_For_EPass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayApprovalPassStatus</servlet-name>
        <servlet-class>DisplayApprovalPassStatus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStatusByAdmin</servlet-name>
        <servlet-class>UpdateStatusByAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>okPassStatus</servlet-name>
        <servlet-class>okPassStatus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register_Servlet</servlet-name>
        <url-pattern>/Register_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApplyForEPass</servlet-name>
        <url-pattern>/ApplyForEPass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Display_user</servlet-name>
        <url-pattern>/Display_user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Applied_For_EPass</servlet-name>
        <url-pattern>/Applied_For_EPass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayApprovalPassStatus</servlet-name>
        <url-pattern>/DisplayApprovalPassStatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStatusByAdmin</servlet-name>
        <url-pattern>/UpdateStatusByAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>okPassStatus</servlet-name>
        <url-pattern>/okPassStatus</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
